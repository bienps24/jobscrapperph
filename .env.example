# ─────────────────────────────────────────────
# REQUIRED
# ─────────────────────────────────────────────

# Kuha sa @BotFather sa Telegram
BOT_TOKEN=your_telegram_bot_token_here

# ─────────────────────────────────────────────
# OPTIONAL
# ─────────────────────────────────────────────

# Gaano kadalas mag-check ng bagong jobs (in minutes)
# Default: 60 (isang oras)
CHECK_INTERVAL_MINUTES=60

# Jooble API Key (libre - mag-register sa jooble.org/api)
# Kung wala, gagamit ng direct scraping
JOOBLE_API_KEY=

# Database path (leave as default for Railway)
DB_PATH=jobs.db
